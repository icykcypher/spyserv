{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "postgres": "Host=localhost;Port=5432;Database=postgres;Username=postgres;Password=new_password"
    },
    "AuthorizationOptions": {
        "RolePermissions": [
            {
                "Role": "Admin",
                "Permissions": [ "Create", "Read", "Update", "Delete" ]
            },
            {
                "Role": "User",
                "Permissions": [ "Read" ]
            }
        ]
    },
    "RabbitMQHost": "localhost",
    "RabbitMQPort": "5672"
}